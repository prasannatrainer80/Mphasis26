<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<bean id="studentOne" class="com.java.spr.Student">
		<property name="sno" value="1" />
		<property name="firstName" value="Sanvi" />
		<property name="lastName" value="s" />
	</bean>
	<bean id="studentTwo" class="com.java.spr.Student">
		<property name="sno" value="2" />
		<property name="firstName" value="Vamsi" />
		<property name="lastName" value="Krishna" />
	</bean>
	<bean id="studentThree" class="com.java.spr.Student">
		<property name="sno" value="3" />
		<property name="firstName" value="Sridhar" />
		<property name="lastName" value="Reddy" />
	</bean>
	<bean id="trainer" class="com.java.spr.Trainer">
		<property name="trainerName" value="Prasanna..." />
		<property name="location" value="Hyderabad" />
		<property name="studentList">
			<list>
				<ref bean="studentTwo"/>
				<ref bean="studentOne"/>
				<ref bean="studentThree"/>
				
			</list>
		</property>
	</bean>
</beans>